<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>



	<g:HTMLPanel>
		<g:AbsolutePanel height="233px">
			<g:at left="232" top="166">
				<g:Button text="Enter" ui:field="login_send" />
			</g:at>
			<g:at left="195" top="69">
				<g:TextBox ui:field="login_textbox" />
			</g:at>
			<g:at left="158" top="76">
				<g:Label text="Login" ui:field="login_label" />
			</g:at>
			<g:at left="195" top="93">
				<g:PasswordTextBox ui:field="pwd_textbox" />
			</g:at>
			<g:at left="129" top="100">
				<g:Label text="Password" />
			</g:at>
			<g:at left="262" top="135">
				<g:Hyperlink text="Subscribe ?" targetHistoryToken="subscribe" />
			</g:at>
			<g:at left="195" top="120">

				<g:Label styleName="wrong_pwd" ui:field="wrong_pwd" />
			</g:at>
			<g:at left="10" top="210">
				<g:Hyperlink text="Check Dnd sample" targetHistoryToken="main"/>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder>
