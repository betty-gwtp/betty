<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
	 <g:AbsolutePanel height="233px">
	 	<g:at left="243" top="156">
	 		<g:Button text="Enter" ui:field="login_send"/>
	 	</g:at>
	 	<g:at left="195" top="69">
	 		<g:TextBox ui:field="login_textbox"/>
	 	</g:at>
	 	<g:at left="158" top="76">
	 		<g:Label text="Login" ui:field="login_label"/>
	 	</g:at>
	 	<g:at left="195" top="93">
	 		<g:PasswordTextBox/>
	 	</g:at>
	 	<g:at left="129" top="100">
	 		<g:Label text="Password"/>
	 	</g:at>
	 	<g:at left="243" top="121">
	 		<g:Hyperlink text="Login forgot ?" targetHistoryToken="newHistoryToken"/>
	 	</g:at>
	 </g:AbsolutePanel>
	 </g:HTMLPanel>
</ui:UiBinder>
