<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:w="urn:import:com.google.gwt.widget.client">

	<g:PopupPanel width="400px" height="300px" styleName="popupPresenter">
		<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER">
			<g:Cell width="100%">
				<g:HorizontalPanel width="100%" styleName="popupPresenterExitBar">
				<g:Label text="Delete Card"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<w:TextButton width="12px" height="10px" text="" ui:field="windowCloseButton">New button</w:TextButton>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Cell width="100%" height="200px" verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel width="98%" height="100%" horizontalAlignment="ALIGN_LEFT">
					<g:Cell width="155px">
						<g:AbsolutePanel width="330px" height="196px">
						<g:at left="0" top="0">
							<g:ScrollPanel width="155px" height="100%">
						<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" ui:field="cardVerticalPanel"/>
					</g:ScrollPanel>
						</g:at>
						<g:at left="168" top="0">
							<g:ScrollPanel height="100%" width="155px">
								<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" ui:field="deleteVerticalPanel"/>
							</g:ScrollPanel>
						</g:at>
					</g:AbsolutePanel>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<w:TextButton text="Delete" ui:field="deleteCardButton">New button</w:TextButton>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel>
					<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER" width="100%" height="50px">
						<w:TextButton text="Close" width="100%" height="" ui:field="closeButton">New button</w:TextButton>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:PopupPanel>
</ui:UiBinder>
