<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:PopupPanel modal="true" glassEnabled="true" animationEnabled="true">


		<g:FormPanel width="351px" height="186px" ui:field="formPanel">
			<g:AbsolutePanel width="100%" height="100%">
				<g:at left="15" top="18">
					<g:Label text="Project Name"/>
				</g:at>
				<g:at left="103" top="10">
					<g:TextBox ui:field="project_name" name="project_name" maxLength="5242" alignment="CENTER"/>
				</g:at>
				<g:at left="32" top="152">
					<g:Button ui:field="cancel_button">Cancel</g:Button>
				</g:at>
				<g:at left="129" top="152">
					<g:Button ui:field="send_button">Create Project</g:Button>
				</g:at>
				<g:at left="103" top="42">
					<g:FileUpload name="file_courses"/>
				</g:at>
				<g:at left="0" top="0">
					<g:Hidden name="sess_id" ui:field="sess_id"/>
				</g:at>
				<g:at left="15" top="47">
					<g:Label text="Courses File"/>
				</g:at>
				
			</g:AbsolutePanel>
			
		</g:FormPanel>



	</g:PopupPanel>
</ui:UiBinder>
