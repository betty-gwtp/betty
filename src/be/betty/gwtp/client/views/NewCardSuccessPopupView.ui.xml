<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:w="urn:import:com.google.gwt.widget.client">

	<g:PopupPanel width="400px" height="220px" styleName="popupPresenter">
		<g:VerticalPanel width="100%" height="100%">
			<g:Cell width="100%">
				<g:HorizontalPanel width="100%" styleName="popupPresenterExitBar">
					<g:Label text="Success"/>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<w:TextButton width="10px" height="12px" text="" ui:field="buttonCloseWindow">New button</w:TextButton>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:Label text="The card has been successfully added"/>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<w:TextButton text="Ok" ui:field="buttonOk">New button</w:TextButton>
			</g:Cell>
		</g:VerticalPanel>
	</g:PopupPanel>
</ui:UiBinder>
