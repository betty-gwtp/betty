<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:w="urn:import:com.google.gwt.widget.client">

	<g:HTMLPanel styleName="test">
		<g:VerticalPanel width="100%" height="100%">
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_BOTTOM">
				<g:Label ui:field="info_label" height="17px" />
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:Image width="64px" height="64px" url="LoadingProject.gif" ui:field="loadingPicutre"/>
			</g:Cell>
			<g:HTMLPanel ui:field="project_field" />
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:Button ui:field="new_project">Create New Poject</g:Button>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
