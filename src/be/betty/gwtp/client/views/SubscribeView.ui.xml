<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:w="urn:import:com.google.gwt.widget.client">

	<g:HTMLPanel width="" height="">
		<g:DockPanel width="100%" height="80%" ui:field="DockSubscribePanel" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_BOTTOM">
			<g:Dock direction="CENTER" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:AbsolutePanel styleName="subscribe" height="250px" width="500px" ui:field="absoluteSubscribePanel">
				<g:at left="101" top="42">
					<g:Label ui:field="userSubLabel" text="Username" horizontalAlignment="ALIGN_RIGHT" width="" height=""/>
				</g:at>
				<g:at left="101" top="77">
					<g:Label ui:field="passSubLabel " text="Password"/>
				</g:at>
				<g:at left="101" top="112">
					<g:Label ui:field="passVerSubLabel" text="Password"/>
				</g:at>
				<g:at left="125" top="147">
					<g:Label text="Email" ui:field="emailSubLabel"/>
				</g:at>
				<g:at left="180" top="36">
					<g:TextBox ui:field="userSubTextbox"/>
				</g:at>
				<g:at left="180" top="71">
					<g:PasswordTextBox ui:field="passSubTextbox"/>
				</g:at>
				<g:at left="180" top="106">
					<g:PasswordTextBox ui:field="passVerSubTextbox"/>
				</g:at>
				<g:at left="180" top="141">
					<g:TextBox ui:field="emailSubTextbox"/>
				</g:at>
				<g:at left="370" top="42">
					<g:Label ui:field="userSubErrorLabel" styleName="errorLabel"/>
				</g:at>
				<g:at left="370" top="77">
					<g:Label ui:field="passSubErrorLabel" styleName="errorLabel"/>
				</g:at>
				<g:at left="370" top="112">
					<g:Label ui:field="passVerSubErrorLabel" styleName="errorLabel"/>
				</g:at>
				<g:at left="370" top="147">
					<g:Label ui:field="emailSubErrorLabel" styleName="errorLabel"/>
				</g:at>
				<g:at left="336" top="32">
					<g:Image width="32px" height="32px" ui:field="usernamePicture"/>
				</g:at>
				<g:at left="336" top="67">
					<g:Image width="32px" height="32px" ui:field="passwordPicture"/>
				</g:at>
				<g:at left="336" top="102">
					<g:Image width="32px" height="32px" ui:field="passwordVerPicture"/>
				</g:at>
				<g:at left="336" top="137">
					<g:Image width="32px" height="32px" ui:field="emailPicture"/>
				</g:at>
				<g:at left="215" top="191">
					<w:TextButton text="Subscribe" ui:field="buttonSubscribe">New button</w:TextButton>
				</g:at>
			</g:AbsolutePanel>
			</g:Dock>
		</g:DockPanel>
	</g:HTMLPanel>
</ui:UiBinder>
