<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:a='urn:import:be.betty.gwtp.client.views.ourWidgets'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:DockPanel width="100%" height="100%"
			horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:Dock direction="NORTH" width="100%" height="20%">
				<g:DockPanel width="100%" height="100%">
					<g:Dock direction="EAST"
						width="20%" height="100%" horizontalAlignment="ALIGN_RIGHT">
						<g:AbsolutePanel width="130px" height="95px"
							styleName="instancePanel">
							<g:at left="0" top="0">
								<g:Label text="0" width="130px" height="57px" ui:field="currentInstance"
									styleName="currentInstanceNumber" horizontalAlignment="ALIGN_CENTER" />
							</g:at>
							<g:at left="10" top="63">
								<g:ListBox ui:field="comboInstance" width="110px"
									height="22px" />
							</g:at>
							<g:at left="98" top="36">
								<g:Image width="22px" height="21px" url="Plus_sign.png" />
							</g:at>
							<g:at left="0" top="0">
								<g:Label text="Instance Courante" width="130px" height="13px"
									styleName="simpleLabel" horizontalAlignment="ALIGN_CENTER" />
							</g:at>
						</g:AbsolutePanel>
					</g:Dock>
					<g:Dock direction="WEST" width="20%" height="100%">
						<g:AbsolutePanel height="150px" ui:field="dndpanel"
							width="225px">
							<g:at left="0" top="0">
								<g:SimplePanel ui:field="CardSelectionOption" width="225px"
							height="150px" />
							</g:at>
						</g:AbsolutePanel>
					</g:Dock>
					<g:Dock direction="CENTER" width="80%" height="100%">
						<g:AbsolutePanel height="150px">
							<g:at left="302" top="70">
								<g:ListBox ui:field="combo_viewChoice1" />
							</g:at>
							<g:at left="399" top="70">
								<g:ListBox ui:field="combo_viewChoice2" />
							</g:at>
							<g:at left="10" top="10">
								<g:Label ui:field="mainLabel" width="100%" height="15%"
									text="Main Label" horizontalAlignment="ALIGN_CENTER" />
							</g:at>
						</g:AbsolutePanel>
					</g:Dock>
				</g:DockPanel>
			</g:Dock>
			<g:Dock direction="SOUTH" width="100%" height="20%">
				<g:ScrollPanel width="100%" height="200px">
					<g:VerticalPanel width="100%" height="100%"
						spacing="2" ui:field="notifBarVerticalPanel" />
				</g:ScrollPanel>
			</g:Dock>
			<g:Dock direction="CENTER" horizontalAlignment="ALIGN_CENTER"
				width="80%" height="60%">
				<g:SimplePanel width="100%" height="100%" ui:field="board_panel" />
			</g:Dock>
			<g:Dock direction="WEST" width="20%" height="60%">
				<g:AbsolutePanel height="650px" width="225px">
					<g:at left="0" top="0">
						<g:ScrollPanel height="507px" width="225px">
							<a:ModifiedVerticalPanel width="100%" height="100%"
								ui:field="cards_panel" horizontalAlignment="ALIGN_CENTER" />
						</g:ScrollPanel>
					</g:at>
				</g:AbsolutePanel>
			</g:Dock>
		</g:DockPanel>


	</g:HTMLPanel>
</ui:UiBinder>
