<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:DockPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:Dock direction="NORTH" width="100%" height="20%">
						<g:DockPanel width="100%" height="100%">
							<g:Dock direction="WEST" verticalAlignment="ALIGN_MIDDLE" width="20%" height="100%">
								<g:AbsolutePanel width="150px" height="110px" styleName="instancePanel">
					<g:at left="10" top="0">
						<g:Label text="0" width="57px" height="63px" ui:field="currentInstance" styleName="currentInstanceNumber"/>
					</g:at>
					<g:at left="0" top="82">
						<g:ListBox ui:field="comboInstance"/>
					</g:at>
					<g:at left="72" top="79">
						<g:Image width="28px" height="21px" url="Plus_sign.png"/>
					</g:at>
					<g:at left="0" top="0">
						<g:Label text="Instance Courante" width="100px" height="13px" styleName="simpleLabel"/>
					</g:at>
				</g:AbsolutePanel>
							</g:Dock>
							<g:Dock direction="EAST" width="20%" height="100%" horizontalAlignment="ALIGN_CENTER">
								<g:AbsolutePanel height="150px" ui:field="dndpanel"
			width="150px">
			<g:at left="0" top="10">
				<g:Label ui:field="mainLabel" width="100%" height="15%" text="Main Label" horizontalAlignment="ALIGN_CENTER" />
			</g:at>
			<g:at left="0" top="31">
				<g:Label width="100%" height="75%" ui:field="content" horizontalAlignment="ALIGN_CENTER" />
			</g:at>
			
			
		</g:AbsolutePanel>
							</g:Dock>
							<g:Dock direction="CENTER">
								<g:AbsolutePanel height="98px">
									<g:at left="302" top="70">
										<g:ListBox ui:field="combo_viewChoice1"/>
									</g:at>
									<g:at left="399" top="70">
										<g:ListBox ui:field="combo_viewChoice2"/>
									</g:at>
								</g:AbsolutePanel>
							</g:Dock>
						</g:DockPanel>
					</g:Dock>
					<g:Dock direction="EAST" width="20%" height="60%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
						<g:VerticalPanel horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_TOP" height="100%" width="20%"/>
					</g:Dock>
					<g:Dock direction="CENTER" horizontalAlignment="ALIGN_CENTER" width="60%" height="60%">
						<g:SimplePanel width="100%" height="100%" ui:field="board_panel"/>
					</g:Dock>
					<g:Dock direction="WEST" width="20%" height="60%" horizontalAlignment="ALIGN_CENTER">
						<g:DecoratedStackPanel width="100%" height="333px">
					<g:SimplePanel width="100%" height="100%" ui:field="CardSelectionOption" g:StackPanel-text="Filters"/>
					<g:ScrollPanel width="100%" height="100%" g:StackPanel-text="Cards" alwaysShowScrollBars="true">
					<g:VerticalPanel width="100%" height="100%"
						spacing="10" ui:field="cards_panel" horizontalAlignment="ALIGN_CENTER" />
				</g:ScrollPanel>
				</g:DecoratedStackPanel>
					</g:Dock>
				</g:DockPanel>


	</g:HTMLPanel>
</ui:UiBinder>
