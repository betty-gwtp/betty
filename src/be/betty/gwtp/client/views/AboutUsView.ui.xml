<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:w="urn:import:com.google.gwt.widget.client">

	<g:PopupPanel width="300px" height="155px" styleName="popupPresenter" animationEnabled="true">
		<g:HTMLPanel width="300px" height="145px">
			<g:VerticalPanel width="100%" verticalAlignment="ALIGN_MIDDLE" height="">
				<g:HorizontalPanel width="100%" styleName="popupPresenterExitBar">
					<g:Label text="About..."/>
					<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_BOTTOM">
						<w:TextButton text="" width="10px" height="12px" ui:field="closeWindowButton">New button</w:TextButton>
					</g:Cell>
				</g:HorizontalPanel>
				<g:ScrollPanel height="85px">
					<g:Label ui:field="aboutUsLabel" text="" width="100%" height="100%"/>
				</g:ScrollPanel>
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" width="100%" height="45px">
					<w:TextButton text="Ok" ui:field="okButton">New button</w:TextButton>
				</g:Cell>
			</g:VerticalPanel>
		</g:HTMLPanel>
		
	</g:PopupPanel>
	
</ui:UiBinder>
