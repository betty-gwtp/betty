<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:w="urn:import:com.google.gwt.widget.client">

	<g:PopupPanel width="250px" height="150px" styleName="popupPresenter">
		<g:VerticalPanel width="100%" height="100%">
			<g:Cell width="100%">
				<g:HorizontalPanel width="100%" styleName="popupPresenterExitBar">
				<g:Label text="Delete card"/>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<w:TextButton text="" width="10px" height="12px" ui:field="buttonCloseWindow">New button</w:TextButton>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:Label text="Are you sure to delete this card?" ui:field="label"/>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:HorizontalPanel spacing="5">
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<w:TextButton text="Yes" ui:field="buttonYes">New button</w:TextButton>
				</g:Cell>
				<w:TextButton text="No" ui:field="buttonNo">New button</w:TextButton>
			</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:PopupPanel>
</ui:UiBinder>
