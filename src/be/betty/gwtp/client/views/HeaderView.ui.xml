<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="HtmlPanel" width="100%" height="20%">
		<hr/>
		<g:DockPanel width="100%" height="40%" verticalAlignment="ALIGN_TOP" horizontalAlignment="ALIGN_CENTER">
			<g:Dock direction="NORTH">
				<g:DockPanel horizontalAlignment="ALIGN_CENTER" width="100%" height="100%" styleName="Header">
					<g:Dock direction="WEST" verticalAlignment="ALIGN_MIDDLE" width="50%" height="100%">
						<g:HTMLPanel width="100%" height="100%">
					<h1> Welcome to Betty</h1>
				</g:HTMLPanel>
					</g:Dock>
					<g:Dock direction="EAST" width="50%" height="100%" horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
						<g:AbsolutePanel width="300px" height="100%">
							<g:at left="10" top="10">
								<g:Label ui:field="loginLabel" text="Jack" horizontalAlignment="ALIGN_RIGHT" width="179px" height="18px"/>
							</g:at>
							<g:at left="195" top="10">
								<g:Button ui:field="deco" text="Deconnection"/>
							</g:at>
						</g:AbsolutePanel>
					</g:Dock>
				</g:DockPanel>
			</g:Dock>
			<g:Dock direction="SOUTH">
				<g:MenuBar animationEnabled="true" width="100%" height="" autoOpen="true" ui:field="menuBar">
				<g:MenuItem text="File">
					<g:MenuBar vertical="true">
						<g:MenuItem text="Open"/>
						<g:MenuItem text="Save"/>
						<g:MenuItemSeparator/>
						<g:MenuItem text="Export Project">
							<g:MenuBar vertical="true">
								<g:MenuItem text="XLS file"/>
								<g:MenuItem text="XML file"/>
							</g:MenuBar>
						</g:MenuItem>
						<g:MenuItem text="Import Project"/>
					</g:MenuBar>
				</g:MenuItem>
				<g:MenuItem text="Edit">
					<g:MenuBar vertical="true">
						<g:MenuItem text="Undo"/>
						<g:MenuItem text="Data">
							<g:MenuBar vertical="true">
								<g:MenuItem text="Professor"/>
								<g:MenuItem text="Room"/>
								<g:MenuItem text="Course"/>
								<g:MenuItem text="Card"/>
							</g:MenuBar>
						</g:MenuItem>
					</g:MenuBar>
				</g:MenuItem>
				<g:MenuItem text="Project">
					<g:MenuBar vertical="true">
						<g:MenuItem text="Solver" ui:field="solverMenu">
					<g:MenuBar vertical="true">
						<g:MenuItem text="Calcule" ui:field="CalculeMenu"/>
					</g:MenuBar>
				</g:MenuItem>
						<g:MenuItem text="Manual">
							<g:MenuBar vertical="true"/>
						</g:MenuItem>
					</g:MenuBar>
				</g:MenuItem>
				<g:MenuItem text="View">
					<g:MenuBar vertical="true">
						<g:MenuItem text="Classic"/>
						<g:MenuItem text="Comparator"/>
					</g:MenuBar>
				</g:MenuItem>
				<g:MenuItemSeparator/>
				<g:MenuItem text="Help" ui:field="helpMenuItem">
					<g:MenuBar vertical="true" ui:field="helpMenuBar">
						<g:MenuItem text="Manual"/>
					</g:MenuBar>
				</g:MenuItem>
			</g:MenuBar>
			</g:Dock>
		</g:DockPanel>
	<g:HTMLPanel ui:field="contentPanel" height="80%" styleName="project" width="50*" />
	</g:HTMLPanel>
	
	
	
</ui:UiBinder>
