<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="HtmlPanel" width="100%" height="20%">
		<hr/>
		<g:DockPanel width="100%" height="40%" verticalAlignment="ALIGN_TOP" horizontalAlignment="ALIGN_CENTER">
			<g:Dock direction="NORTH">
				<g:DockPanel horizontalAlignment="ALIGN_CENTER" width="100%" height="100%" styleName="Header">
					<g:Dock direction="EAST" horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
						<g:Button ui:field="deco" text="Deconnection"/>
					</g:Dock>
					<g:Dock direction="WEST" verticalAlignment="ALIGN_MIDDLE">
						<g:HTMLPanel width="" height="">
					<h1> Welcome to Betty</h1>
				</g:HTMLPanel>
					</g:Dock>
				</g:DockPanel>
			</g:Dock>
			<g:Dock direction="SOUTH">
				<g:MenuBar animationEnabled="true" width="100%" height="">
				<g:MenuItem text="File">
					<g:MenuBar vertical="true"/>
				</g:MenuItem>
				<g:MenuItemSeparator/>
				<g:MenuItem text="Help">
					<g:MenuBar vertical="true">
						<g:MenuItem text="About us"/>
					</g:MenuBar>
				</g:MenuItem>
			</g:MenuBar>
			</g:Dock>
		</g:DockPanel>
	<g:HTMLPanel ui:field="contentPanel" height="80%" styleName="project" width="50*" />
	</g:HTMLPanel>
	
	
	
</ui:UiBinder>
