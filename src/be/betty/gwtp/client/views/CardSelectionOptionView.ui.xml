<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<g:AbsolutePanel height="285px" width="233px" styleName="panel_filter_options">
			<g:at left="10" top="52">
				<g:Label text="Option" width="73px" height="13px" horizontalAlignment="ALIGN_LEFT"/>
			</g:at>
			<g:at left="89" top="86">
				<g:ListBox width="118px" height="18px" ui:field="group_choice"/>
			</g:at>
			<g:at left="89" top="47">
				<g:ListBox ui:field="comboBoxFilterType" width="118px" height="18px"/>
			</g:at>
			<g:at left="10" top="91">
				<g:Label text="More" width="73px" height="13px" horizontalAlignment="ALIGN_LEFT"/>
			</g:at>
			<g:at left="10" top="10">
				<g:Label text="Filtre de selection des cartons" styleName="small_title"/>
			</g:at>
			<g:at left="10" top="138">
				<g:ScrollPanel width="213px" height="137px" ui:field="cardFilterScrollPanel">
					<g:VerticalPanel width="100%" height="100%" ui:field="cardFilterVerticalPanel"/>
				</g:ScrollPanel>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder>
